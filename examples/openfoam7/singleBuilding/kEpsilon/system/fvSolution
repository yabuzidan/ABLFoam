FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        tolerance       1e-9;
        relTol          0.001;

        smoother        GaussSeidel;

        nCellsInCoarsestLevel 100;
    }

    "(U|k|epsilon|omega|nut)"
    {
        solver          smoothSolver; 
        smoother        GaussSeidel;
        tolerance       1e-9; 
        relTol          0.001;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;

    residualControl
    {
        "(p|U|k|epsilon|omega|nut)" 1e-5;
    }
}

relaxationFactors
{
    p               0.2;
    U               0.5;
    k               0.5;
    epsilon         0.5;
    omega           0.5;
    nut             0.5;
}


// ************************************************************************* //
