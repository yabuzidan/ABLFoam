convertToMeters 1;

// Domain dimensions (m)
xMin   -1;
xMax   3;
yMin   -0.75;
yMax   0.75;
zMin   0;
zMax   1.5;

// Building dimensions (m)
H      0.125;  //height
B      0.15;   //width
D      0.1;   //depth

// Number of grid divisions on buidling

nCell1   8;   // 0.5* streamwise (x) divisions (/2) on side faces of building
nCell2   8;  // 0.5 * lateral (y) divisions on front and back faces of buidling
nCell3   15;   // 0.5* veritcal (z) divisions (/2) along building height

// Number of grid divisions of domain

nCell4   15;  // x divisions upstream of building
nCell5   20;  // x divisions downstream of building
nCell6   15;  // lateral (y) divisions of domain either side of building
nCell7   15;  // veritcal (z) divisions from buidling roof to top boundary of domain


Hdiv2 #calc "0.5*$H";
Bdiv2 #calc "0.5*$B";
negBdiv2 #calc "-0.5*$B";
Ddiv2 #calc "0.5*$D";
negDdiv2 #calc "-0.5*$D";


